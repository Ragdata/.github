    },
    "support": {
		"issues": {
			"label": "issues",
			"url": "https://github.com/ragdata/.github/issues"
		},
		"discussions": {
			"label": "discussions",
			"url": "https://github.com/ragdata/.github/discussions"
		}
    },
	"dependencies": {
		"git": {
			"name": "Git",
			"check": "what -q git",
			"install": "sudo apt install -y git"
		},
		"yq": {
			"name": "YQ",
			"check": "what -q yq",
			"install": "wget https://github.com/mikefarah/yq/releases/download/4.30.8/yq_linux_amd64.tar.gz -O - | tar xz && mv yq_linux_amd64 /usr/bin/yq"
		},
		"moreutils": {
			"name": "MoreUtils",
			"check": "what -q sponge",
			"install": "sudo apt install -y moreutils"
		}
	}
}
