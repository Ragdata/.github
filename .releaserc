    "git": {
        "user": {
            "name": "Ragdata",
            "email": "ragdata@users.noreply.github.com"
        }
    },
    "files": [
        "package.json",
        "README.md",
        "COPYRIGHT"
    ],
    "homepage": "https://ragdata.github.io/.github",
    "repository": {
        "label": "GitHub Repository",
        "url": "https://github.com/ragdata/.github"
    },
    "author": {
        "name": "Darren (Ragdata) Poulton",
        "email": "ragdata@users.noreply.github.com",
